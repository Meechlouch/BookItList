<br>
<div>
    <h2 id="user-name">Welcome Username!</h2>
    <p>Books on your BookitList</p>
    <a href="/search" class="btn btn-primary btn-md">
        <span class="fas fa-book"></span> Search for Books</a>
</div>
<br>
<div class="row">
    <div class="notBooked col-md-5">
        <h3>Bookit List</h3>
        {{#each books}}
        {{#unless bookedIt}}
        {{> books/not-bookedIt-block bookedIt=false}}
        {{/unless}}
        {{/each}}
    </div>
    <div class="col-md-2"></div>
    <div class="booked col-md-5">
        <h3>Booked It List</h3>
        <ul>
            {{#each books}}
            {{#if bookedIt}}
            {{> books/bookedIt-block bookedIt=true}}
            {{/if}}
            {{/each}}
        </ul>
    </div>
</div>
